### Genealogy

@prefix sdh-shacl: <https://sdhss.org/shacl/profiles/> . 
@prefix sh: <http://www.w3.org/ns/shacl#> .  
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .  
@prefix sdh-short: <https://sdhss.org/ontology/shortcuts/P1> .
@prefix sdh: <https://sdhss.org/ontology/core/> .
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> . 


sdh-shacl:sdh_C13_Shape a sh:NodeShape;

    sh:targetClass sdh:C13;
    sh:name "Geographical Place";

    sh:property [
        sh:path rdfs:label;
        sh:name "has name";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
    ];

    sh:property [
        sh:path rdfs:comment;
        sh:name "has definition";
        sh:datatype xsd:string;
        sh:order 2;
        sh:minCount 1;
        sh:maxCount 1;
    ];   
        
    . 


sdh-shacl:sdh_C11_Shape a sh:NodeShape;
    sh:targetClass sdh:C11;
    sh:name "Gender";

    sh:property [
        sh:path rdfs:label;
        sh:name "has name";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
    ];

    sh:property [
        sh:path rdfs:comment;
        sh:name "has definition";
        sh:datatype xsd:string;
        sh:order 2;
        sh:minCount 1;
        sh:maxCount 1;
    ];   

    .


sdh-shacl:crm_E21_Shape a sh:NodeShape;
    sh:targetClass crm:E21;
    sh:name "Person";

    sh:property [
        sh:path rdfs:label;
        sh:name "has name";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
    ];

    sh:property [
        sh:path rdfs:comment;
        sh:name "has definition";
        sh:datatype xsd:string;
        sh:order 2;
        sh:minCount 1;
        sh:maxCount 1;
    ];   

    sh:property [
        sh:path sdh:P23;
        sh:name "has gender";
        sh:class sdh:C11;
        sh:order 3;
    ];   

    sh:property [
        sh:path [ sh:inversePath crm:P98; ];
        sh:name "was born";
        sh:order 4;
        sh:maxCount 1;
    ];

    .


sdh-shacl:crm_E67_Shape a sh:NodeShape;
    sh:targetClass crm:E67;
    sh:name "Birth";

    sh:property [
        sh:path rdfs:label;
        sh:name "has label";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
    ];

    sh:property [
        sh:path rdfs:comment;
        sh:name "has definition";
        sh:datatype xsd:string;
        sh:order 2;
    ];   
    
    sh:property [
        sh:path sdh-short:P1;
        sh:name "occured in";
        sh:datatype xsd:string;
        sh:order 3;
        sh:maxCount 1;
    ];
        
    sh:property [
        sh:path crm:P8;
        sh:name "took place in";
        sh:class sdh:C13;
        sh:order 4;
        sh:maxCount 1;
    ];
            
    sh:property [
        sh:path crm:P98;
        sh:name "brought into life";
        sh:class crm:E21;
        sh:maxCount 1;
    ];

    sh:property [
        sh:path sdh:P22;
        sh:name "stemmed from";
        sh:class sdh:C3;
        sh:maxCount 1;
    ];

    . 


sdh-shacl:sdh_C3_Shape a sh:NodeShape;
    sh:targetClass sdh:C3;
    sh:name "Relationship";

    sh:property [
        sh:path rdfs:label;
        sh:name "has label";
        sh:datatype xsd:string;
        sh:order 1;
        sh:minCount 1;
        sh:maxCount 1;
    ];

    sh:property [
        sh:path rdfs:comment;
        sh:name "has definition";
        sh:datatype xsd:string;
        sh:order 2;
    ];   
    
    sh:property [
        sh:path sdh:P20;
        sh:name "had partner";
        sh:class crm:E21;
        sh:order 3;
    ];

    sh:property [
        sh:path [ sh:inversePath sdh:P22;];
        sh:name "resulted in";
    ];

    . 